login = ''

while not login:
    login = input('enter login : ')

password =''

while not password:
    password = input('enter the password : ')


    if login == 'slava' and password == '12345':
        print('hello slava')
    elif login == 'lisa' and password == 'qwerty':
        print('hello lisa')
    else:
        print('dostypa net')

